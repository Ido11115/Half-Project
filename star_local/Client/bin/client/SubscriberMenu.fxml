<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="client.SubscriberMenuController" spacing="10"
	alignment="CENTER" style="-fx-padding: 20;">
	<Label text="Subscriber Menu"
		style="-fx-font-size: 18px; -fx-font-weight: bold;" />

	<Button text="Search Book" onAction="#handleSearchBook" />
	<Button text="My Profile" onAction="#handleMyProfile" />
	<Button text="Logout" onAction="#handleLogout" />
</VBox>
